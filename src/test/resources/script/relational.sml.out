fun listFields lists =
  from {a = x, b = y} in lists
  yield x + 1;
val listFields = fn : {a:int, b:'a} list -> int list

listFields [];
val it = [] : int list

listFields [{a = 1, b = 2}, {a = 3, b = 0}, {a = 4, b = 5}];
val it = [2,4,5] : int list


