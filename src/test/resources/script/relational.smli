(*) Pass 'take' and 'skip' via function arguments
let
  fun earlyEmps n =
    from e in emps
      yield {e.id, e.deptno}
      order id
      skip n - 2
      take n
in
  (earlyEmps 2, earlyEmps 3)
end;
> val it =
>   ([{deptno=10,id=100},{deptno=20,id=101}],
>    [{deptno=20,id=101},{deptno=30,id=102},{deptno=30,id=103}])
>   : {deptno:int, id:int} list * {deptno:int, id:int} list

