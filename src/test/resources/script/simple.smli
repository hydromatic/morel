(* Patterns ---------------------------------------------------- *)

(*) The following example is from "Introduction to Standard ML" by Robert Harper.
val x = (( "foo", true ), 17 );
> val x = (("foo",true),17) : string * bool * int
val ( l as (ll,lr), r ) = x;
> val l = ("foo",true) : string * bool
> val ll = "foo" : string
> val lr = true : bool
> val r = 17 : int

Sys.set ("matchCoverageEnabled", false);
> val it = () : unit
fun f (true, []) = ~1
  | f (true, l as (hd :: tl)) = length l
  | f (false, list) = 0;
> val f = fn : bool * 'a list -> int
f (true, ["a","b","c"]);
> val it = 3 : int
Sys.unset "matchCoverageEnabled";
> val it = () : unit

let
  val w as (x, y) = (1, 2)
in
  #1 w + #2 w + x + y
end;
> val it = 6 : int

val x as (y, z) = (1, 2);
> val x = (1,2) : int * int
> val y = 1 : int
> val z = 2 : int
val x as h :: t = [1,2,3];
> val x = [1,2,3] : int list
> val h = 1 : int
> val t = [2,3] : int list
val x as y as (h,i) :: t = [(1,2),(3,4),(5,6)];
> val x = [(1,2),(3,4),(5,6)] : (int * int) list
> val y = [(1,2),(3,4),(5,6)] : (int * int) list
> val h = 1 : int
> val i = 2 : int
> val t = [(3,4),(5,6)] : (int * int) list

val a as SOME (b as (c, d)) = SOME (1, 2);
> val a = SOME (1,2) : (int * int) option
> val b = (1,2) : int * int
> val c = 1 : int
> val d = 2 : int

val x as y as z = 3;
> val x = 3 : int
> val y = 3 : int
> val z = 3 : int

(* Field access ------------------------------------------------ *)

(*) Record field access using accessor
val r = {a = 1, b = "two"};
> val r = {a=1,b="two"} : {a:int, b:string}
#a r;
> val it = 1 : int
#b r;
> val it = "two" : string

(*) Record field access using dot notation
r.a;
> val it = 1 : int
r.b;
> val it = "two" : string

(*) Tuple field access using accessor
val t = (42, "hello", 3.14);
> val t = (42,"hello",3.14) : int * string * real
#1 t;
> val it = 42 : int
#2 t;
> val it = "hello" : string
#3 t;
> val it = 3.14 : real

(*) Tuple field access using dot notation
t.1;
> val it = 42 : int
t.2;
> val it = "hello" : string
t.3;
> val it = 3.14 : real

(*) Inline tuple with dot notation
(1, "x").1;
> val it = 1 : int
(1, "x").2;
> val it = "x" : string

(*) Nested tuple access
val nested = ((1, 2), (3, 4));
> val nested = ((1,2),(3,4)) : int * int * (int * int)
nested.1;
> val it = (1,2) : int * int
(*) Chained numeric field access requires parentheses because '1.1' is a real literal
(nested.1).1;
> val it = 1 : int
(nested.1).2;
> val it = 2 : int
(nested.2).1;
> val it = 3 : int

(*) Mixed record and tuple access
val mixed = {pair = (10, 20), name = "test"};
> val mixed = {name="test",pair=(10,20)} : {name:string, pair:int * int}
mixed.pair;
> val it = (10,20) : int * int
mixed.pair.1;
> val it = 10 : int
mixed.pair.2;
> val it = 20 : int

(*) Chained mixed access: tuple field, record field, tuple field
val t2 = (({x = (100, 200)}, "a"), "b");
> val t2 = (({x=(100,200)},"a"),"b") : {x:int * int} * string * string
(*) Chained numeric field access requires parentheses
((t2.1).1).x.2;
> val it = 200 : int

(*) Syntactic ambiguity: real literals vs tuple access
(*) 3.1 is a real literal, not tuple access
3.1;
> val it = 3.1 : real

(*) 3.2e1 is scientific notation (32.0)
3.2e1;
> val it = 32.0 : real

(*) To access field of literal tuple, use parentheses
(3, "three").1;
> val it = 3 : int

(*) Spaces around dot
val tup = ("a", "b");
> val tup = ("a","b") : string * string
tup.1;
> val it = "a" : string
tup . 1;
> val it = "a" : string
tup. 1;
> val it = "a" : string
tup .1;
> val it = "a" : string

(*) Unit values have no fields
();
> val it = () : unit
{};
> val it = () : unit
().1;
> stdIn:1.1-1.3 Error: reference to field 1 of non-record type unit
>   raised at: stdIn:1.1-1.3
{}.1;
> stdIn:1.1-1.3 Error: reference to field 1 of non-record type unit
>   raised at: stdIn:1.1-1.3

(*) Singleton record with numeric field name (not a tuple)
{1 = "hello"};
> val it = {1="hello"} : {1:string}
{1 = "hello"}.1;
> val it = "hello" : string
{1 = "hello"}.2;
> stdIn:1.15 Error: no field '2' in type '{1:string}'
>   raised at: stdIn:1.15

(*) Space disambiguates: '3 . 1' applies '.1' to integer 3 (type error)
3 . 1;
> stdIn:1.1 Error: reference to field 1 of non-record type int
>   raised at: stdIn:1.1

(*) Error: tuple indices are 1-based
(1, 2).0;
> stdIn:1.8 Error: no field '0' in type 'int * int'
>   raised at: stdIn:1.8

(*) Error: field does not exist
(1, 2, 3).4;
> stdIn:1.11 Error: no field '4' in type 'int * int * int'
>   raised at: stdIn:1.11

