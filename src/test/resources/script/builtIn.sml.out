(*) val substring : string * int * int -> string
String_substring;
val it = fn : string * int * int -> string

String_substring("hello, world", 2, 7);
val it = "llo, wo" : string

String_substring("hello, world", 0, 1);
val it = "h" : string

String_substring("hello", 5, 0);
