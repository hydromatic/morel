(*) val all : ('a -> bool) -> 'a list -> bool
List_all;
val it = fn : ('a -> bool) -> 'a list -> bool

List_all (fn x => x mod 2 = 0) [1,3,5];
val it = false : bool

List_all (fn x => x mod 2 = 0) [2,4,6];
val it = true : bool

List_all (fn x => x mod 2 = 0) [1,2,3];
val it = false : bool

List_all (fn x => x mod 2 = 0) [];
