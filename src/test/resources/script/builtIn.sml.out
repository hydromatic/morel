plusOne o timesTwo;
val it = fn : int -> int

(plusOne o timesTwo) 3;
val it = 7 : int

plusOne o timesTwo o plusThree;
val it = fn : int -> int

((plusOne o timesTwo) o plusThree) 3;
val it = 13 : int

(plusOne o (timesTwo o plusThree)) 3;
val it = 13 : int


